---
apiVersion: v1
kind: Service
metadata:
  name: token-service
  namespace: {{ registry_operator_namespace }}
  labels:
    app: token-service
spec:
  selector:
    app: token-service
  ports:
    - name: http
      port: {{ registry_operator_token_http_port }}
      targetPort: 8080
    - name: https
      port: {{ registry_operator_token_https_port }}
      targetPort: 8443
  type: ClusterIP
