---
kind: StorageClass
apiVersion: storage.k8s.io/v1
metadata:
  name: {{ sc_name_1000 }}
provisioner: efs.csi.aws.com
parameters:
  provisioningMode: efs-ap
  fileSystemId: {{ aws_efs_filesystem_id }}
  directoryPerms: "755"
  uid: "1000"
  gid: "1000"
  basePath: "/dynamic_provisioning" # optional
