apiVersion: v1
kind: Service
metadata:
    name: image-validation-admission-svc
    namespace: {{ image_validating_webhook_namespace }}
    labels:
        name: image-validation-admission
spec:
    ports:
        - name: webhook
          port: 443
          targetPort: {{ image_validating_webhook_target_port }}
    selector:
        app: image-validation-admission